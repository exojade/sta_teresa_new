	<queryString>
		<![CDATA[SELECT vehicle.MTOP_NO, concat(vehicle.MTOP_NO,'-',YEAR(curdate())) as mtop_year, (SELECT city_admin from settings LIMIT 1 ) as city_ad,(SELECT city_admin_pos from settings LIMIT 1 ) as city_ad_pos, (SELECT head from settings LIMIT 1 ) as head1, (SELECT head_pos from settings LIMIT 1 ) as head_pos,(SELECT head_office from settings LIMIT 1 ) as head_office ,(SELECT mayor from settings LIMIT 1 ) as mayor1, CONCAT(firstname,' ',lastname) as  fullname , address, plate_no, make, chassis_no, motor_no,
vehicle.plate_no1, vehicle.make1, vehicle.motor_no1, vehicle.chassis_no1, vehicle.yearModel1,
vehicle.plate_no, vehicle.make, vehicle.motor_no, vehicle.chassis_no, vehicle.yearModel,
IFNULL(DATE_FORMAT(expiration_date,'%b %d %Y'),'') as exp, or_no, DATE_FORMAT(date_paid,'%b %d %Y') as paid, substitution_fee FROM vehicle, operator, fees WHERE vehicle.MTOP_NO = operator.MTOP_NO AND vehicle.MTOP_NO = fees.MTOP_NO AND vehicle.MTOP_NO = $P{mtop_no}  ORDER BY fees.ID DESC LIMIT 1]]>
