	<queryString>
		<![CDATA[SELECT vehicle.MTOP_NO, concat(vehicle.MTOP_NO,'-',YEAR(curdate())) as mtop_year, (SELECT city_admin from settings LIMIT 1 ) as city_ad,(SELECT city_admin_pos from settings LIMIT 1 ) as city_ad_pos, (SELECT mayor from settings LIMIT 1 ) as city_mayor,(SELECT mayors_permit_fee from settings LIMIT 1 ) as mayors, (SELECT sticker from settings LIMIT 1 ) as stick, CONCAT(firstname,' ',lastname) as  fullname , address, plate_no, make, chassis_no, motor_no, reg_no, status, released_date, IFNULL(DATE_FORMAT(expiration_date,'%b %d %Y'),'') as exp, or_no, DATE_FORMAT(date_paid,'%b %d %Y') as paid, mayors_permit_fee, (SELECT head from settings LIMIT 1 ) as head1, (SELECT head_pos from settings LIMIT 1 ) as head_pos, (SELECT head_office from settings LIMIT 1 ) as head_office FROM vehicle, operator, fees WHERE vehicle.MTOP_NO = operator.MTOP_NO AND vehicle.MTOP_NO = fees.MTOP_NO AND vehicle.MTOP_NO = $P{mtop_no} ORDER BY fees.ID DESC LIMIT 1]]>
